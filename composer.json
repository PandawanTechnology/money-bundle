{
    "name": "pandawan-technology/money-bundle",
    "description": "Integrates pandawan-technology/money into Symfony' applications",
    "license": "MIT",
    "authors": [
        {
            "name": "Xavier Coureau",
            "email": "xavier@pandawan-technology.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "pandawan-technology/money": "dev-main",
        "symfony/dependency-injection": "^4.4||^5.0",
        "symfony/config": "^4.4||^5.0",
        "symfony/http-kernel": "^4.4||^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.x-dev",
        "friendsofphp/php-cs-fixer": "dev-master",
        "symfony/form": "^4.4||^5.0"
    },
    "autoload": {
        "psr-4": {"PandawanTechnology\\MoneyBundle\\": "" },
        "exclude-from-classmap": [
            "/Resources/",
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "PandawanTechnology\\MoneyBundle\\Tests\\": "Tests/"
        }
    },
    "replace": {
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*"
    },
    "minimum-stability": "dev"
}
